{
  "name": "loki-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "reg-viz": "nodemon reg-viz.js",
    "server": "nodemon ./dev-tools/server/index.js",
    "serve-storybook": "concurrently \"yarn server\" \"yarn storybook\"",
    "test-cmd": "concurrently",
    "reg": "reg-cli ./.storybook/__snapshots__/current/ ./.storybook/__snapshots__/reference/ ./.storybook/__snapshots__/diff/ -R ./.storybook/__snapshots__/report.html -J ./.storybook/__snapshots__/reg.json"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "devDependencies": {
    "@octokit/core": "4.1.0",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-vite": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.14",
    "express": "^4.18.2",
    "loki": "^0.32.0",
    "nodemon": "^3.0.1",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "puppeteer": "^21.3.7",
    "reg-keygen-git-hash-plugin": "^0.12.2",
    "reg-notify-github-plugin": "^0.12.2",
    "reg-publish-s3-plugin": "^0.12.2",
    "reg-suit": "^0.12.2",
    "storybook": "^7.4.6",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
